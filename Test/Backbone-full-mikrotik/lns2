/interface ethernet
set [ find default-name=ether1 ] disable-running-check=no
set [ find default-name=ether2 ] disable-running-check=no
set [ find default-name=ether3 ] disable-running-check=no
set [ find default-name=ether4 ] disable-running-check=no
set [ find default-name=ether5 ] disable-running-check=no
set [ find default-name=ether6 ] disable-running-check=no
set [ find default-name=ether7 ] disable-running-check=no
set [ find default-name=ether8 ] disable-running-check=no
/interface vlan
add interface=ether1 name=Interco_LNS1 vlan-id=100
/port
set 0 name=serial0
/routing bgp instance
add as=64555 disabled=no name=BGP_BACKBONE
/routing bgp template
add hold-time=1m keepalive-time=30s name=TPL_BACKBONE nexthop-choice=force-self output.redistribute=connected,static
/routing isis instance
add afi=ip areas=49.0001 disabled=no name=ISIS_BACKONE system-id=1000.1000.1002
/ip address
add address=100.124.0.2 interface=lo network=100.124.0.2
add address=100.125.0.2/30 interface=Interco_LNS1 network=100.125.0.0
/routing bgp connection
add instance=BGP_BACKBONE local.address=100.124.0.2 .role=ibgp name=LNS2 remote.address=100.124.0.1 templates=TPL_BACKBONE
/routing isis interface-template
add instance=ISIS_BACKONE interfaces=Interco_LNS1 levels=l2
add instance=ISIS_BACKONE interfaces=lo levels=l2
/system identity
set name=LNS2
